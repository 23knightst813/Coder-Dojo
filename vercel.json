{
  "version": 2,
  "builds": [
    {
      "src": "flaskr/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "flaskr/app.py"
    }
  ],
  "build": {
    "env": {
      "PIPENV_VERBOSITY": "-1"
    },
    "install": "pip install -r flaskr/requirements.txt"
  }
}
